{
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "devCommand": "vite dev",
  "rewrites": [
    {
      "source": "/apod-images/:path*",
      "destination": "https://apod.nasa.gov/:path*"
    },
    {
      "source": "/api-nasa/:path*",
      "destination": "https://images-api.nasa.gov/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
